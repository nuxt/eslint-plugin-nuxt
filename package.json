{
  "name": "eslint-plugin-nuxt",
  "version": "2.0.0",
  "description": "ESLint plugin for Nuxt.js",
  "author": "Clark Du <clarkdo@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "homepage": "https://github.com/nuxt/eslint-plugin-nuxt#readme",
  "bugs": {
    "url": "https://github.com/nuxt/eslint-plugin-nuxt/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt/eslint-plugin-nuxt.git"
  },
  "keywords": [
    "nuxt",
    "eslint",
    "eslintplugin"
  ],
  "files": [
    "lib",
    "!__tests__"
  ],
  "scripts": {
    "test": "jest --silent",
    "lint": "eslint ./",
    "release": "standard-version",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "devDependencies": {
    "eslint": "^8.1.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "7",
    "jest": "^27.3.1",
    "lint-staged": "^10.5.4",
    "pinst": "^2.1.6",
    "standard-version": "^9.3.2"
  },
  "dependencies": {
    "eslint-plugin-vue": "^7.20.0",
    "semver": "^7.3.5",
    "vue-eslint-parser": "^8.0.0"
  }
}
