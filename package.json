{
  "name": "eslint-plugin-nuxt",
  "version": "4.0.0",
  "description": "ESLint plugin for Nuxt.js",
  "author": "Clark Du <clarkdo@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "homepage": "https://github.com/nuxt/eslint-plugin-nuxt#readme",
  "bugs": {
    "url": "https://github.com/nuxt/eslint-plugin-nuxt/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt/eslint-plugin-nuxt.git"
  },
  "keywords": [
    "nuxt",
    "eslint",
    "eslintplugin"
  ],
  "files": [
    "lib",
    "!__tests__"
  ],
  "scripts": {
    "test": "jest --silent",
    "lint": "eslint ./",
    "release": "standard-version",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^13.3.0",
    "pinst": "^3.0.0",
    "standard-version": "^9.3.2"
  },
  "dependencies": {
    "eslint-plugin-vue": "^9.23.0",
    "semver": "^7.6.0",
    "vue-eslint-parser": "^9.4.2"
  },
  "packageManager": "yarn@3.8.1"
}
