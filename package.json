{
  "name": "eslint-plugin-nuxt",
  "version": "2.0.0",
  "description": "ESLint plugin for Nuxt.js",
  "author": "Clark Du <clarkdo@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "homepage": "https://github.com/nuxt/eslint-plugin-nuxt#readme",
  "bugs": {
    "url": "https://github.com/nuxt/eslint-plugin-nuxt/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt/eslint-plugin-nuxt.git"
  },
  "keywords": [
    "nuxt",
    "eslint",
    "eslintplugin"
  ],
  "files": [
    "lib",
    "!__tests__"
  ],
  "scripts": {
    "test": "jest --silent",
    "lint": "eslint ./",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "standard-version": "^9.1.0"
  },
  "dependencies": {
    "eslint-plugin-vue": "^7.5.0",
    "semver": "^7.3.4",
    "vue-eslint-parser": "^7.4.1"
  }
}
